{
  "name": "adriantoine.com",
  "version": "1.0.0",
  "description": "Personal website",
  "main": "index.js",
  "scripts": {
    "clean":
      "rm -rf public/bundle* && rm -rf public/appcache && rm -rf public/sw-offline-google-analytics.prod.* && rm -rf public/sw.js && rm -rf public/style* && rm -f public/index.html",
    "build":
      "npm run clean && npm run copy-google-sw && webpack --config webpack/prod.config.js",
    "copy-google-sw":
      "cp node_modules/sw-offline-google-analytics/build/importScripts/sw-offline-google-analytics.prod.* public/",
    "start": "webpack-dev-server --config webpack/dev.config.js",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:adriantoine/adriantoine.com.git"
  },
  "engines": {
    "node": "8.x.x",
    "npm": "5.x.x"
  },
  "author": "Adrien Antoine",
  "license": "ISC",
  "homepage": "https://github.com/adriantoine/adriantoine.com#readme",
  "dependencies": {
    "autoprefixer": "^7.1.4",
    "buble-loader": "^0.4.1",
    "classnames": "^2.1.3",
    "css-loader": "^0.28.7",
    "cssnano": "^3.0.1",
    "eslint": "^4.18.2",
    "eslint-config-prettier": "^2.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-preact": "^1.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.3.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "firebase-tools": "^3.4.0",
    "html-webpack-plugin": "^2.28.0",
    "lost": "^8.0.0",
    "normalize.css": "^7.0.0",
    "offline-plugin": "^4.6.1",
    "postcss-calc": "^6.0.0",
    "postcss-color-function": "^4.0.0",
    "postcss-css-variables": "^0.8.0",
    "postcss-custom-media": "^6.0.0",
